[Unit]
Description=AlphaGenesis Dashboard Service
After=network.target

[Service]
Type=simple
User=outerwoolf
WorkingDirectory=/home/outerwoolf/AlphaGenesis
Environment=PATH=/home/outerwoolf/AlphaGenesis/venv/bin:/usr/local/bin:/usr/bin:/bin
ExecStart=/home/outerwoolf/AlphaGenesis/venv/bin/python dashboard/app.py
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target 